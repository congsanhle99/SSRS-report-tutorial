  <DataSets>
    <DataSet Name="DS_Prod">
      <Query>
        <DataSourceName>DS_DDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        DimProducts.ProductName, FactSale.Quantity, FactSale.UnitPrice, FactSale.SalesAmount, DimSuppliers.Region
FROM            DimProducts INNER JOIN
                         FactSale ON DimProducts.ProductKey = FactSale.ProductKey INNER JOIN
                         DimSuppliers ON DimProducts.SupplierKey = DimSuppliers.SupplierKey
Where DimSuppliers.Region = @Region OR @Region is NULL</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="UnitPrice">
          <DataField>UnitPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSET_Region">
      <Query>
        <DataSourceName>DS_DDS</DataSourceName>
        <CommandText>SELECT DISTINCT Region
FROM            DimSuppliers</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
