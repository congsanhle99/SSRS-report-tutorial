    <DataSet Name="DSET_ShowParameter">
      <Query>
        <DataSourceName>DS_DDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT   DimSuppliers.Region, DimSuppliers.Country, DimSuppliers.CompanyName, DimCategories.CategoryName, DimProducts.ProductName, DimProducts.QuantityPerUnit, FactSale.Quantity, FactSale.UnitPrice
FROM            DimSuppliers INNER JOIN
                         DimProducts ON DimSuppliers.SupplierKey = DimProducts.SupplierKey INNER JOIN
                         DimCategories ON DimProducts.CategoryKey = DimCategories.CategoryKey INNER JOIN
                         FactSale ON DimProducts.ProductKey = FactSale.ProductKey
Where DimSuppliers.Region = @Region</CommandText>
      </Query>
